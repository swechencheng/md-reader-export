{
    "manifest_version": 3,
    "name": "Markdown Reader HTML Export",
    "version": "1.0.0",
    "description": "Extension that exports the Markdown Reader rendered page to html",
    "icons": {
        "16": "icons/md-reader-export.16x16.icon.png",
        "48": "icons/md-reader-export.48x48.icon.png",
        "128": "icons/md-reader-export.128x128.icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "main.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "externally_connectable": {
        "ids": [
            "medapdbncneneejhbgcjceippjlfkmkg"
        ]
    }
}